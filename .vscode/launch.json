{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch WileyWidget (Local)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0-windows/WileyWidget.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    {
      "name": "Launch WileyWidget (Debug Exceptions)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0-windows/WileyWidget.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "justMyCode": false,
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SYNCFUSION_LICENSE_KEY": "${env:SYNCFUSION_LICENSE_KEY}",
        "COREHOST_TRACE": "1"
      },
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Launch WileyWidget (Azure)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0-windows/WileyWidget.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Production",
        "AZURE_SQL_SERVER": "${env:AZURE_SQL_SERVER}",
        "AZURE_SQL_DATABASE": "${env:AZURE_SQL_DATABASE}",
        "AZURE_SQL_USER": "${env:AZURE_SQL_USER}",
        "AZURE_SQL_PASSWORD": "${env:AZURE_SQL_PASSWORD}"
      },
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Attach to Process",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    },
    {
      "name": "Debug Syncfusion WPF Form",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/bin/Debug/net9.0-windows/WileyWidget.exe",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "SYNCFUSION_LICENSE_KEY": "${env:SYNCFUSION_LICENSE_KEY}"
      },
      "envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Debug Python Startup Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/dev-start-debug.py",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "DEBUG_STARTUP": "true",
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": ["--verbose"]
    },
    {
      "name": "Debug Python Startup (Clean Only)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/dev-start-debug.py",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "DEBUG_STARTUP": "true",
        "PYTHONPATH": "${workspaceFolder}"
      },
      "args": ["--clean-only", "--verbose"]
    },
    {
      "name": "Debug Python Environment Loader",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/load-env.py",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--status"]
    },
    {
      "name": "Debug Python Cleanup Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/cleanup-dotnet.py",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--dry-run"]
    },
    {
      "name": "Debug All Python Tests",
      "type": "debugpy",
      "request": "launch",
      "module": "unittest",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "args": ["discover", "-v", "-s", "${workspaceFolder}"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug Python Tests with Coverage",
      "type": "debugpy",
      "request": "launch",
      "module": "coverage",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "args": [
        "run",
        "--source=${workspaceFolder}",
        "-m",
        "unittest",
        "discover",
        "-v",
        "-s",
        "${workspaceFolder}"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug Current Python Test File",
      "type": "debugpy",
      "request": "launch",
      "module": "unittest",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "args": ["-v", "${file}"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug C# Tests",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/WileyWidget.Tests/bin/Debug/net9.0/WileyWidget.Tests.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Debug C# UI Tests",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/WileyWidget.UiTests/bin/Debug/net9.0/WileyWidget.UiTests.dll",
      "args": [],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Debug C# Tests with Coverage",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/WileyWidget.Tests/bin/Debug/net9.0/WileyWidget.Tests.dll",
      "args": [
        "--collect:\"XPlat Code Coverage\"",
        "--results-directory:${workspaceFolder}/TestResults/Coverage"
      ],
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "COVERAGE_FILE": "${workspaceFolder}/TestResults/Coverage/coverage.cobertura.xml"
      }
    },
    {
      "name": "Python: Attach to debugpy",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 5678
      },
      "pathMappings": [
        {
          "localRoot": "${workspaceFolder}",
          "remoteRoot": "${workspaceFolder}"
        }
      ],
      "justMyCode": false
    },
    {
      "name": "Debug Startup with debugpy",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/dev-start-debugpy.py",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--debug-port", "5678", "--timing"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "DEBUG_STARTUP": "true"
      },
      "justMyCode": false,
      "stopOnEntry": false
    },
    {
      "name": "Debug Startup (No Wait)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/dev-start-debugpy.py",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--debug-port", "5678", "--no-wait", "--timing"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "DEBUG_STARTUP": "true"
      },
      "justMyCode": false,
      "stopOnEntry": false
    },
    {
      "name": "Debug WPF Threading Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/debug-wpf-thread.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--verbose", "--timeout=90"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "DEBUG_STARTUP": "true"
      }
    },
    {
      "name": "Debug DI Threading Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/test-di-thread.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--verbose"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug Test Thread Monitor",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/monitor-test-thread.py",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--verbose", "--timeout=90"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug Test Resource Monitor",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/monitor-test-resources.py",
      "python": "${workspaceFolder}/.venv/Scripts/python.exe",
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "args": ["--report", "--timeout=60"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug WPF Threading Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/debug-wpf-thread.py",
      "args": ["--verbose", "--timeout=90"],
      "env": {
        "PYTHONPATH": "${workspaceFolder}",
        "DEBUG_STARTUP": "true"
      },
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "Debug DI Threading Script",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/test-di-thread.py",
      "args": ["--verbose"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    },
    {
      "name": "Debug Test Thread Monitor",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/monitor-test-thread.py",
      "args": ["--verbose", "--timeout=90"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      }
    }
  ]
}
