{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Wiley Widget (Full Symbols + Just My Code OFF)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/WileyWidget.WinForms/bin/Debug/net9.0-windows/WileyWidget.WinForms.exe",

      "args": [],
      "cwd": "${workspaceFolder}/src/WileyWidget.WinForms",

      "symbolOptions": {
        "searchPaths": [
          "${workspaceFolder}",
          "https://nuget.smbsrc.net/symbols",
          "https://symbols.nuget.org/download/symbols"
        ],
        "searchMicrosoftSymbolServer": true,
        "cachePath": "${workspaceFolder}/.vsdbg-cache"
      },

      "justMyCode": false,

      "stopAtEntry": false,

      "logging": {
        "exceptions": true,
        "moduleLoad": false,
        "programOutput": true
      },

      "sourceFileMap": {
      },

      "internalConsoleOptions": "neverOpen"
    },

    {
      "name": "Attach to Wiley Widget (running)",
      "type": "coreclr",
      "request": "attach",
      "processName": "WileyWidget.WinForms.exe",
      "justMyCode": false
    }
  ]
}
