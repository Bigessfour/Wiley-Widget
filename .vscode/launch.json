{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wiley Widget WinForms – Full Symbols + Hot Reload",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build WileyWidget.WinForms",
      "program": "${workspaceFolder}/src/WileyWidget.WinForms/bin/Debug/net9.0-windows10.0.26100.0/WileyWidget.WinForms.exe",
      "args": [],
      "cwd": "${workspaceFolder}/src/WileyWidget.WinForms",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "env": {
        "DOTNET_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/__w/Wiley-Widget/Wiley-Widget": "${workspaceFolder}"
      },
      "symbolOptions": {
        "searchPaths": [
          "${workspaceFolder}/**",
          "https://symbols.nuget.org/download/symbols",
          "https://msdl.microsoft.com/download/symbols",
          "C:\\SyncfusionSymbols**",
          "${env:USERPROFILE}\\.nuget\\packages"
        ],
        "searchMicrosoftSymbolServer": true,
        "searchNuGetOrgSymbolServer": true,
        "cachePath": "${workspaceFolder}/.vsdbg"
      },
      "sourceLinkOptions": {
        "*": {
          "enabled": true
        }
      },
      "justMyCode": false,
      "requireExactSource": false,
      "enableStepFiltering": false,
      "logging": {
        "module": true,
        "engineLogging": false
      }
    },
    {
      "name": "Attach Native (cppvsdbg)",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "stopAtEntry": false
    }
  ],
  "compounds": [
    {
      "name": "Wiley Widget - Managed + Native",
      "configurations": [
        "Wiley Widget WinForms – Full Symbols + Hot Reload",
        "Attach Native (cppvsdbg)"
      ]
    }
  ]
}
