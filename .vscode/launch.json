{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Wiley Widget WinForms – Full Symbols + Hot Reload",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build WileyWidget.WinForms",
      "program": "${workspaceFolder}/src/WileyWidget.WinForms/bin/Debug/net10.0-windows10.0.26100.0/WileyWidget.WinForms.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/WileyWidget.WinForms",
      "stopAtEntry": false,
      "console": "integratedTerminal",
      "env": {
        "DOTNET_ENVIRONMENT": "Development",
        "_NT_SYMBOL_PATH": "",
        "_NT_ALT_SYMBOL_PATH": ""
      },
      "sourceFileMap": {
        "/__w/Wiley-Widget/Wiley-Widget": "${workspaceFolder}"
      },
      "symbolOptions": {
        "searchPaths": [
          "${workspaceFolder}",
          "${workspaceFolder}/.symbols",
          "${env:USERPROFILE}\\.nuget\\packages"
        ],
        "searchMicrosoftSymbolServer": false,
        "searchNuGetOrgSymbolServer": false,
        "cachePath": "${workspaceFolder}/.vsdbg"
      },
      "sourceLinkOptions": {
        "*": {
          "enabled": true
        }
      },
      "justMyCode": true,
      "requireExactSource": false,
      "enableStepFiltering": false,
      "logging": {
        "moduleLoad": false,
        "engineLogging": false
      }
    },
    {
      "name": "Attach Native (cppvsdbg)",
      "type": "cppvsdbg",
      "request": "attach",
      "processId": "${command:pickProcess}",
      "stopAtEntry": false
    }
  ],
  "compounds": [
    {
      "name": "Wiley Widget - Managed + Native",
      "configurations": [
        "Wiley Widget WinForms – Full Symbols + Hot Reload",
        "Attach Native (cppvsdbg)"
      ]
    }
  ]
}
