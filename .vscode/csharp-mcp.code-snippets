      "    {",
      "        Console.WriteLine(\"⏳ Fetching data...\");",
      "        await Task.Delay(100); // Simulate async operation",
      "        ",
      "        ${4:return \"Data\";}",
      "    }",
      "}",
      "",
      "var service = new ${1:MyService}();",
