  "csharp-mcp-budget-entry": {
    "prefix": "csx-budget",
    "description": "Budget entry model for Wiley Widget",
    "body": [
      "using System;",
      "",
      "public class BudgetEntry",
      "{",
      "    public string Department { get; set; }",
      "    public string Category { get; set; }",
      "    public decimal Budgeted { get; set; }",
      "    public decimal Actual { get; set; }",
      "    ",
      "    public decimal Variance => Actual - Budgeted;",
      "    public decimal VariancePercent => Budgeted != 0 ? (Variance / Budgeted) * 100 : 0;",
      "    ",
      "    public override string ToString()",
      "    {",
      "        return $\"{Department} / {Category}: Budgeted=${Budgeted:N0}, Actual=${Actual:N0}, Variance=${Variance:N0} ({VariancePercent:F1}%)\";",
      "    }",
      "}",
      "",
      "var entry = new BudgetEntry",
      "{",
      "    Department = \"${1:Police}\",",
      "    Category = \"${2:Personnel}\",",
      "    Budgeted = ${3:1000000}m,",
      "    Actual = ${4:950000}m",
      "};",
      "",
      "Console.WriteLine(entry);",
      "return entry.Variance;"
    ]
  }
}
