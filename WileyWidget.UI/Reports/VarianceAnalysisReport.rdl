    <Height>2in</Height>
    <Style />
  </Body>
  <Width>7in</Width>
  <Page>
    <LeftMargin>1in</LeftMargin>
    <RightMargin>1in</RightMargin>
    <TopMargin>1in</TopMargin>
    <BottomMargin>1in</BottomMargin>
    <Style />
  </Page>
  <DataSources>
    <DataSource Name="VarianceAnalysisDataSource">
      <ConnectionProperties>
        <DataProvider>Object</DataProvider>
        <ConnectString>WileyWidget.Models.VarianceAnalysis</ConnectString>
      </ConnectionProperties>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="VarianceAnalysisDataSet">
      <Query>
        <DataSourceName>VarianceAnalysisDataSource</DataSourceName>
        <CommandText />
      </Query>
      <Fields>
        <Field Name="Category">
          <DataField>Category</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Budgeted">
          <DataField>Budgeted</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Actual">
          <DataField>Actual</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Variance">
          <DataField>Variance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="VariancePercent">
          <DataField>VariancePercent</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
</Report>