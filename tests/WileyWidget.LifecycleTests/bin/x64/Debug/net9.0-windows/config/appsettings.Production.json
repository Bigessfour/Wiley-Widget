{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=WileyWidgetDb;Trusted_Connection=True;MultipleActiveResultSets=true",
    "AzureConnection": "${AZURE_SQL_CONNECTION_STRING}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "WileyWidget": "Information"
    }
  },
  "Database": {
    "EnableSensitiveDataLogging": false,
    "EnableDetailedErrors": false,
    "CommandTimeout": 30,
    "MaxRetryCount": 3,
    "MaxRetryDelay": "00:00:30"
  },
  "Azure": {
    "SubscriptionId": "${AZURE_SUBSCRIPTION_ID}",
    "TenantId": "${AZURE_TENANT_ID}",
    "SqlServer": "${AZURE_SQL_SERVER}",
    "Database": "${AZURE_SQL_DATABASE}",
    "KeyVault": {
      "Url": "${AZURE_KEY_VAULT_URL}"
    }
  },
  "QuickBooks": {
    "ClientId": "${QBO_CLIENT_ID}",
    "ClientSecret": "${QBO_CLIENT_SECRET}",
    "RedirectUri": "${QBO_REDIRECT_URI}",
    "Environment": "${QBO_ENVIRONMENT:production}"
  },
  "Syncfusion": {
    "LicenseKey": "${SYNCFUSION_LICENSE_KEY}",
    "KeyVaultSecretName": "Syncfusion-LicenseKey"
  },
  "Application": {
    "Environment": "Production",
    "EnableDeveloperExceptionPage": false,
    "EnableHsts": true
  }
}
