      <dependentAssembly>
        <assemblyIdentity name="Intuit.Ipp.Diagnostics" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-14.7.0.3" newVersion="14.7.0.2" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Intuit.Ipp.EntitlementService" culture="neutral" />
