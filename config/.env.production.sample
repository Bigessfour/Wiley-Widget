# Production environment variables for WileyWidget
# Copy to .env.production and fill with correct values

# Azure SQL (used by DatabaseConfiguration)
AZURE_SQL_SERVER=busbuddy-server.database.windows.net
AZURE_SQL_DATABASE=BusBuddy

# Syncfusion
SYNCFUSION_LICENSE_KEY=your-syncfusion-license-key

# QuickBooks
QUICKBOOKS_CLIENT_ID=
QUICKBOOKS_CLIENT_SECRET=
QUICKBOOKS_REDIRECT_URI=
QUICKBOOKS_ENVIRONMENT=Sandbox

# Azure Key Vault (kept for future use; app currently prefers env vars)
AZURE_KEY_VAULT_URL=https://your-vault.vault.azure.net/
AZURE_KEY_VAULT_NAME=your-vault

# App
ASPNETCORE_ENVIRONMENT=Production
